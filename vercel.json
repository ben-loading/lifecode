  "framework": "nextjs",
  "regions": ["sin1"],
  "functions": {
    "src/app/api/report/generate/route.ts": {
      "maxDuration": 120
    }
  },
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase-url",
